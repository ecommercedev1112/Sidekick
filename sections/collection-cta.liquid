						<!-- Calendly inline widget begin -->
						<div class="calendly-inline-widget" data-url="https://calendly.com/r-r-client-service/personalized-call" style="min-width:320px;height:700px;"></div>
						<script type="text/javascript" src="https://assets.calendly.com/assets/external/widget.js" async></script>
