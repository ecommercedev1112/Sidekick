				<script>
					document.getElementById('open-modal').onclick = function() {
						document.body.classList.add('no-scroll');
						document.getElementById('calendly-modal').style.display = 'block';
					};
					document.getElementById('open-modal-1').onclick = function() {
						document.body.classList.add('no-scroll');
						document.getElementById('calendly-modal').style.display = 'block';
					};

					document.getElementsByClassName('close-button')[0].onclick = function() {
						document.body.classList.remove('no-scroll');
						document.getElementById('calendly-modal').style.display = 'none';
					};
					document.getElementsByClassName('close-button-bg')[0].onclick = function() {
						document.body.classList.remove('no-scroll');
						document.getElementById('calendly-modal').style.display = 'none';
					};

					// Close the modal if the user clicks outside of the modal content
					window.onclick = function(event) {
						var modal = document.getElementById('calendly-modal');
						if (event.target == modal) {
							document.body.classList.remove('no-scroll');
							modal.style.display = 'none';
						}
					};
				</script>

				<script>
					    // Delegated Close Button Event
						document.body.addEventListener('click', function(event) {
							if (event.target.matches('.close-button')) {
								document.body.classList.remove('no-scroll');
								document.getElementById('calendly-modal').style.display = 'none';
							}
						});

						// Click Outside to Close
						window.addEventListener('click', function(event) {
							var modal = document.getElementById('calendly-modal');
							if (event.target === modal) {
								document.body.classList.remove('no-scroll');
								modal.style.display = 'none';
							}
						});
				</script>
				
