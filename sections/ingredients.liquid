<style>
	.specific-img-ingredients {
		position: absolute;
		top: 0;
		bottom: 0;
		width: 200%;
		left: -50%;
		height: 100%;
		transform: scale(2.5);
		object-fit: contain;
	}

	.how-to-use-wrapper h1 {
		font-size: 55px;
		font-style: normal;
		font-weight: 700;
	}

	.how-to-use-wrapper .jc-faq-page-content-heading {
		color: #2D2926;
		font-family: Poppins;
		font-size: 16px;
		font-style: normal;
		font-weight: 500;
		line-height: normal;
		letter-spacing: 1.28px;
		text-transform: uppercase;
	}

	.cta-wrapper-ingredients {
		display: flex;
		padding-right: max(calc((100vw - 1380px) / 2), 27px);
		padding-top: 0px;
		padding-bottom: 60px;
		background: #fff;
		overflow: hidden;
	}

	.cta-img-wrapper {

		height: 100%;
		margin-right: 0px;
		margin-left: 0px;
		padding: 0px;
		/*
	width: 60%;
    max-width: 820px;
	*/
		text-align: right;
		overflow: visible;
		width: 100%;
		max-width: 504px;

	}

	.cta-img-ingredients {
		margin-bottom: 0px;
		width: 100%;
		object-fit: cover;
		/*
	max-width: 813.84px;
	*/
		height: 750px;
		border-radius: 0px 0px 200px 0px;
	}

	.cta-text-wrapper-ingredients {
		width: 100%;
		max-width: none;
		margin-right: 0px;
		padding-left: 0px;
		text-align: left;
		padding-right: 0px;
		padding-top: 70px;
	}

	.cta-wrapper-ingredients .how-to-use-wrapper {
		margin-left: auto;
	}

	.button-green {
		padding: 21px 30px 19px 30px;
		background-color: #004943;
		-webkit-transition: background-color .25s ease;
		transition: background-color .25s ease;
		font-family: poppins;
		color: #fff;
		text-align: center;
		letter-spacing: 2px;
		text-transform: uppercase;
		border-radius: 50px;
		width: auto;
		line-height: 1em;
		font-size: 16px;
		font-weight: 400;
		text-decoration: none;
	}

	.button-green:hover {
		background-color: white;
		color: #004943;
	}

	.ingredient-wrapper {
		display: flex;
		column-gap: 34px;
		margin-top: 50px;
	}

	.ingredient-image {
		width: 13vw;
		height: 13vw;
		max-width: 140px;
		max-height: 140px;
		min-width: 120px;
		min-height: 120px;
		border-radius: 50%;
	}

	.ingredient-heading {
		color: var(--rr-green-current-web, #004943);
		font-family: century-old-style-std;
		font-size: 26px;
		line-height: 30px;
		font-style: normal;
		font-weight: 400;
		line-height: normal;
		margin-top: 20px;
	}

	.ingredient-item {
		width: 25%;
	}

	.ingredient-item p {
		font-size: 17px;
	}

	.top-heading-wrapper p {
		font-size: 17px;
		font-weight: 400;
		line-height: 26px;
	}

	.cta-img-ingredients.right-rounded.mobile {
		display: none;
	}

	.jc-faq-page-content-wrapper.ingredient-mobile {
		display: none;
	}

	@media (max-width: 1023px) {
		.cta-wrapper-ingredients {
			flex-direction: column-reverse;
			padding-bottom: 40px;
		}
		.cta-wrapper-ingredients .jc-faq-page-content-heading{
			display: flex;
			align-items: center;
		}
		.cta-wrapper-ingredients .cta-img-wrapper.specific-img-ingredients-wrap {
			height: 300px;
			overflow: visible;
		}

		.specific-img-ingredients {

			object-fit: contain;
			position: absolute;
			width: 100%;
			height: 100%;
			left: 0;
			top: -30px;
			transform: scale(2);
		}

		.cta-text-wrapper-ingredients {
			width: 100%;
			max-width: 100%;
		}

		.cta-img-wrapper img {
			border-bottom-left-radius: 0;
			border-bottom-right-radius: 100px;
		}

		.cta-text-wrapper-ingredients {
			padding-left: 20px;
			padding-right: 20px;
			padding-top: 0px;
		}

		.cta-wrapper-ingredients {
			flex-direction: column;
		}

		.cta-img-ingredients.right-rounded {
			display: none;
		}

		.cta-img-ingredients.right-rounded.mobile {
			display: block;
			width: 100%;
			max-width: 100%;
			height: auto;
		}

		.cta-wrapper-ingredients .cta-img-wrapper {
			min-height: auto;
			width: 100%;
			height: auto;
			max-width: none;
		}

		.cta-wrapper-ingredients {
			padding-right: 0px;
		}

	}

	@media (max-width: 767px) {
		.cta-text-wrapper-ingredients h1.h1-new {
